{
  "name": "analyst",
  "displayName": "Data Analyst Agent",
  "version": "1.0.0",
  "description": "AI agent specialized in data analysis, visualization, and insights generation using LangGraph workflows",
  "role": "SYSTEM",
  "avatar": "https://api.dicebear.com/7.x/bottts/svg?seed=analyst",
  "capabilities": [
    "Data cleaning and preprocessing",
    "Statistical analysis",
    "Data visualization",
    "Pattern recognition",
    "Predictive modeling",
    "Report generation"
  ],
  "langGraphSpec": {
    "workflow": "analysis_workflow",
    "nodes": [
      {
        "id": "data_validator",
        "type": "agent",
        "description": "Validates and cleans input data"
      },
      {
        "id": "exploratory_analyzer",
        "type": "agent",
        "description": "Performs exploratory data analysis"
      },
      {
        "id": "statistical_processor",
        "type": "agent",
        "description": "Runs statistical tests and calculations"
      },
      {
        "id": "pattern_detector",
        "type": "agent",
        "description": "Identifies patterns and anomalies in data"
      },
      {
        "id": "visualizer",
        "type": "agent",
        "description": "Creates charts and visualizations"
      },
      {
        "id": "insight_generator",
        "type": "agent",
        "description": "Generates insights and recommendations"
      }
    ],
    "edges": [
      {"from": "data_validator", "to": "exploratory_analyzer"},
      {"from": "exploratory_analyzer", "to": "statistical_processor"},
      {"from": "statistical_processor", "to": "pattern_detector"},
      {"from": "pattern_detector", "to": "visualizer"},
      {"from": "visualizer", "to": "insight_generator"}
    ],
    "conditionalEdges": [
      {
        "from": "data_validator",
        "condition": "data_issues_found",
        "to": "data_validator"
      },
      {
        "from": "pattern_detector",
        "condition": "needs_more_analysis",
        "to": "statistical_processor"
      }
    ]
  },
  "tools": [
    "pandas_processor",
    "numpy_calculator",
    "matplotlib_plotter",
    "seaborn_visualizer",
    "scipy_stats",
    "scikit_learn",
    "plotly_interactive"
  ],
  "configuration": {
    "supported_formats": ["csv", "json", "xlsx", "parquet", "sql"],
    "max_data_size": "100MB",
    "visualization_types": ["line", "bar", "scatter", "heatmap", "box"],
    "confidence_level": 0.95
  },
  "implementation": "TODO: Implement LangGraph workflow for data analyst agent"
} 